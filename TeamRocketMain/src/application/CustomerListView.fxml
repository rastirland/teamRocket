            <Button fx:id="UserDelete1" layoutX="14.0" layoutY="259.0" mnemonicParsing="false" onAction="#deleteCustomers" prefHeight="25.0" prefWidth="107.0" text="Delete " />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
