<AnchorPane prefHeight="210.0" prefWidth="234.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.customerListController">
   <children>
      <AnchorPane layoutX="-4.0" layoutY="-2.0" prefHeight="310.0" prefWidth="700.0">
         <children>
            <Label layoutX="290.0" layoutY="-10.0" prefHeight="72.0" prefWidth="186.0" text="View Customers">
               <font>
                  <Font size="22.0" />
               </font></Label>
            <Button fx:id="userDisplay" layoutX="12.0" layoutY="126.0" mnemonicParsing="false" onAction="#getCustomers" prefHeight="27.0" prefWidth="107.0" text="View Customer" />
            <Button layoutX="605.0" layoutY="258.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="27.0" prefWidth="65.0" text="Back" />
