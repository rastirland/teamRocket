            <TextField fx:id="searchField" layoutX="14.0" layoutY="48.0" onAction="#searchBar" prefHeight="27.0" prefWidth="170.0" />
            <Button fx:id="searchBtn" layoutX="35.0" layoutY="75.0" mnemonicParsing="false" onAction="#searchBar" prefHeight="27.0" prefWidth="124.0" text="Search" />
